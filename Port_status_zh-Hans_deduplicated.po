msgid ""
msgstr "Content-Type: text/plain; charset=UTF-8"

# 4IceG Port Status 特有翻译
# 以下翻译不在 luci-base 中，是 Port Status 扩展功能专用

# 端口标签编辑功能
msgid "Edit Port Label"
msgstr "编辑端口标签"

msgid "Enter new label for this port:"
msgstr "输入此端口的新标签："

msgid "Original label"
msgstr "原始标签"

msgid "Restore Original"
msgstr "恢复原始"

msgid "Click to edit label"
msgstr "点击编辑标签"

msgid "Label max 9 chars, description max 50 chars."
msgstr "标签最多 9 个字符，描述最多 50 个字符。"

# 端口描述功能
msgid "Enter description (optional):"
msgstr "输入描述（可选）："

# 拖拽排序功能
msgid "Hold to drag and reorder"
msgstr "按住拖动以重新排序"

msgid "Port order saved"
msgstr "端口顺序已保存"

# 配置保存功能
msgid "Port configuration saved successfully"
msgstr "端口配置保存成功"

msgid "User settings are saved to the /etc/user_defined_ports.json file."
msgstr "用户设置保存到 /etc/user_defined_ports.json 文件。"

msgid "Saving configuration..."
msgstr "正在保存配置..."

# 配置文件导入导出
msgid "Save .json file"
msgstr "保存 .json 文件"

msgid "Upload .json file"
msgstr "上传 .json 文件"

msgid "Configuration file downloaded"
msgstr "配置文件已下载"

msgid "Configuration file not found"
msgstr "未找到配置文件"

msgid "Download error: %s"
msgstr "下载错误：%s"

# 配置恢复功能
msgid "Restore configuration"
msgstr "恢复配置"

msgid "This will overwrite current ports configuration. Continue?"
msgstr "这将覆盖当前端口配置。继续吗？"

msgid "Configuration restored successfully. Reloading..."
msgstr "配置恢复成功。正在重新加载..."

msgid "File restore failed: %s"
msgstr "文件恢复失败：%s"

msgid "Invalid configuration format"
msgstr "无效的配置格式"

msgid "Invalid JSON file: %s"
msgstr "无效的 JSON 文件：%s"

msgid "Restore"
msgstr "恢复"

# 错误处理
msgid "Save Error"
msgstr "保存错误"

msgid "Cannot save port configuration. "
msgstr "无法保存端口配置。"

msgid "Directory /etc may be read-only or insufficient permissions. "
msgstr "目录 /etc 可能为只读或权限不足。"

msgid "Try running: chmod 755 /etc && touch %s && chmod 644 %s"
msgstr "尝试运行：chmod 755 /etc && touch %s && chmod 644 %s"

msgid "Original error: %s"
msgstr "原始错误：%s"

msgid "Warning: Could not create port configuration file."
msgstr "警告：无法创建端口配置文件。"

msgid "Port customizations will not be saved."
msgstr "端口自定义设置将不会被保存。"

msgid "Check /etc directory permissions"
msgstr "检查 /etc 目录权限"
